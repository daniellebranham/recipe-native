{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "airbnb",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "quotes": [2, "single", { "avoidEscape": true }],
    "prefer-destructuring": 0,
    "indent": "off",
    "no-unused-vars": "warn",
    "comma-dangle": "off",
    "spaced-comment": "off",
    "vars-on-top": "off"
  }
}
